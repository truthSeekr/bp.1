<%- model_class = Kurs -%>
<div class="page-header">
  <h1><%=t '.title', :default => Kurs %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:nazwa) %></dt>
      <dd><pre class="prettyprint"><%= @kurs.nazwa %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:termin) %></dt>
      <dd><pre class="prettyprint"><%= @kurs.termin %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:miejsce) %></dt>
      <dd><pre class="prettyprint"><%= @kurs.miejsce %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:opis) %></dt>
      <dd><pre class="prettyprint"><%= @kurs.opis %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              kurs_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_kurs_path(@kurs), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              kurs_path(@kurs),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

