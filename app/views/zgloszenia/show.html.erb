<%- model_class = Zgloszenia -%>
<div class="page-header">
  <h1><%=t '.title', :default => Zgloszenia %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:imie) %></dt>
      <dd><pre class="prettyprint"><%= @zgloszenia.imie %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:nazwisko) %></dt>
      <dd><pre class="prettyprint"><%= @zgloszenia.nazwisko %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:miasto) %></dt>
      <dd><pre class="prettyprint"><%= @zgloszenia.miasto %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:adres) %></dt>
      <dd><pre class="prettyprint"><%= @zgloszenia.adres %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:telefon) %></dt>
      <dd><pre class="prettyprint"><%= @zgloszenia.telefon %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:email) %></dt>
      <dd><pre class="prettyprint"><%= @zgloszenia.email %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:data_urodzenia) %></dt>
      <dd><pre class="prettyprint"><%= @zgloszenia.data_urodzenia %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:wiadomosc) %></dt>
      <dd><pre class="prettyprint"><%= @zgloszenia.wiadomosc %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zaliczka) %></dt>
      <dd><pre class="prettyprint"><%= @zgloszenia.zaliczka %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:oplacony) %></dt>
      <dd><pre class="prettyprint"><%= @zgloszenia.oplacony %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              zgloszenia_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_zgloszenia_path(@zgloszenia), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              zgloszenia_path(@zgloszenia),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

